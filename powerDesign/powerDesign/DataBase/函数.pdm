       b.ID,
       b.C_NAME,
       b.I_OPER_ID,
       b.C_OPER_NAME,
       b.D_INSERT_DATE,
       b.I_OPER_MODIFYID,
       b.C_OPER_MODIFYNAME,
       b.D_MODIFY_DATE,
       b.I_DEL,
       b.C_ALIASCODE,
       case
