<a:Procedure.Text>create or replace function [%QUALIFIER%]%FUNC%(nv NUMBER) return varchar2 deterministic as
Result number :=0;
n integer := 0 ;
i integer := 3;
r varchar(50) := &#39;999,999,999,999,999&#39;;
vl number;
begin
   if (nv is null) or (nv =&#39;&#39;) then
    vl := 0;
  else
    vl := nv;
  end if;  

  select count(1) into n from NF_NUMBERLENGTH where c_name=&#39;单价位数&#39;;
  if n = 1 then begin
     select n_number into i from NF_NUMBERLENGTH where c_name=&#39;单价位数&#39;;
  end ;end if;
  Result := round(vl , i);
  n := 0;
  
  while n&lt;i loop begin
    if n = 0 then begin
        r := r||&#39;.0&#39;;
    end ; else begin
        r := r||&#39;0&#39;;
    end ; end if;
    n := n+1;    
  end; end loop;
  return trim(to_char(Result , r));
end;
</a:Procedure.Text>
<a:Procedure.Function>1</a:Procedure.Function>
