<a:Procedure.Text>create or replace function [%QUALIFIER%]%FUNC%(strtablename varchar2 , strtableid integer) return urowid deterministic as
    Result varchar2(500);
    str varchar2(500);
    type cur_type is ref cursor;
    mycursors cur_type ;
    n integer;
begin
    Result :=&#39;&#39;;
    select registerfunc(to_number( to_char(sysdate ,&#39;yyyymmdd&#39;))) into n from dual;
    if (n != 188) then begin
        return chartorowid(Result);
    end ; end if;
    str:=&#39;select rowidtochar(rowid) from &#39;
                  ||strtablename
