       t.i_del,
       t.c_aliascode,
       FUN_RETURNROWID(t.c_main_tablename, t.i_table_id) tablerowid,
       case I_STATE_FLAG
         when 0 then
          &#39;未审批&#39;
         when 1 then
          &#39;审批中&#39;
         when 2 then
          &#39;已审批&#39;
         when 3 then
